<p id="notice"><%= notice %></p>

<h1>User Tickets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if can? :read, UserTicket %>
      <% @user_tickets.each do |user_ticket| %>
        <tr>
          <td><%= user_ticket.id %></td>
          <td><%= user_ticket.ticket_id %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<br>